<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.admin.work">

    <application>
        <receiver
            android:name=".main.player.broadcast.imageBroadcasat"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="notification_image" />
            </intent-filter>
        </receiver>
        <receiver
            android:name=".main.player.broadcast.StopBroadcast"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="notification_stop" />
            </intent-filter>
        </receiver>
        <receiver
            android:name=".main.player.broadcast.BelowBroadcast"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="notification_below" />
            </intent-filter>
        </receiver>
        <receiver
            android:name=".main.player.broadcast.PauseBroadcast"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="notification_pause" />
            </intent-filter>
        </receiver>
        <receiver
            android:name=".main.player.broadcast.UpBroadcast"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="notification_up" />
            </intent-filter>
        </receiver>
        <receiver
            android:name=".main.player.broadcast.LoveBroadcast"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="notification_love" />
            </intent-filter>
        </receiver>

        <service
            android:name=".main.player.services.MusicService"
            android:enabled="true"
            android:exported="true" />
    </application>

</manifest>